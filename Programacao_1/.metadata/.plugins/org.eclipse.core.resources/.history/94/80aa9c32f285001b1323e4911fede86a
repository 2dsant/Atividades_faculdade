public class FluxoRepetição {

	public static void main(String[] args) {
		
		int controle = 0;
		
		while(controle < 10) {
			System.out.println("Controle: " + controle);
			controle++;
		}
		do {
			System.out.println("Controle: " + controle);
		} while (controle < 1);
		
		
		for (int controle = 0; controle <= 10;  controle++) {
			System.out.println("Controle: " + controle);
		}

	}

}